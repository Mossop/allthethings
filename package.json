  "scripts": {
    "clean": "rm -rf dist",
    "build-schema": "graphql-codegen",
    "build": "yarn build-schema && yarn build-client && yarn build-server",
    "build-client": "webpack",
    "watch-client": "webpack --watch",
    "build-server": "tsc -b modules/server/init",
    "watch-server": "tsc -b --watch modules/server/init",
    "watch": "npm-run-all -p watch-client watch-server",
    "start": "node dist/server/init testing/config.json",
    "jslint": "eslint .",
    "prettier": "prettier --write .",
    "depcheck": "depcheck",
    "lint": "yarn prettier && yarn jslint && yarn depcheck"
  },
  "imports": {
