  "scripts": {
    "clean": "rm -rf dist",
    "build-schema": "graphql-codegen && node build-schema.js",
    "watch-schema": "graphql-codegen -w",
    "build-client": "webpack",
    "watch-client": "webpack --watch",
    "build-db": "tsc -b modules/db",
    "build-server": "tsc -b modules/server/init",
    "watch-server": "tsc -b --watch modules/server/init",
    "build": "yarn build-schema && yarn build-server && yarn build-client",
    "watch": "npm-run-all -p watch-schema watch-client watch-server",
    "start": "node dist/server/init testing/config.json",
    "jslint": "eslint modules/client && eslint modules/db && eslint modules/schema && eslint modules/server && eslint modules/services && eslint modules/utils && eslint *.js",
    "prettier": "prettier --write .",
    "depcheck": "depcheck",
    "lint": "yarn prettier && yarn jslint && yarn depcheck",
    "test": "jest"
  },
